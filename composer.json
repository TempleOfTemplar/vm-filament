{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "awcodes/filament-curator": "^0.5.0",
        "based/laravel-typescript": "^0.0.3",
        "bezhansalleh/filament-shield": "^2.2",
        "doctrine/dbal": "^3.4",
        "filament/filament": "^2.0",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "filament/spatie-laravel-tags-plugin": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "infyomlabs/adminlte-templates": "^5.0",
        "infyomlabs/laravel-generator": "^5.0",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^9.4",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "meilisearch/meilisearch-php": "^0.24.2",
        "overtrue/laravel-favorite": "^5.0",
        "rahmanramsi/filament-editorjs": "1.0.1",
        "rawilk/laravel-form-components": "^7.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/vkontakte": "^5.0",
        "spatie/laravel-tags": "^4.3",
        "spatie/laravel-typescript-transformer": "^2.1",
        "templeoftemplar/filament-quill": "dev-master",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.13",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "post-update-cmd": [
        "@php artisan filament:upgrade"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
